{
  "name": "Chaining-Tracker",
  "displayName": "Chaining Tracker",
  "description": "chaining tracking",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.50.0"
  },
  "activationEvents": [
    "onCommand:extension.chainingTracker"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.chainingTracker",
        "title": "Chaining Tracker"
      }
    ]
  },
  "publisher": "eonduck2",
  "repository": {"url": "https://github.com/eonduck2/chaining-tracker.git"},
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.50.0",
    "typescript": "^4.0.3",
    "eslint": "^7.10.0"
  },
  "license":"MIT"
}
